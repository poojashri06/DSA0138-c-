# DSA0138-c-
#include<iostream>
#include<string>
using namespace std;
class student {
	private:
		string name;
		public:
			student(string name1="unknown"):
				name(name1)
				
				
				{
				}
				void printName(){
					cout<<name<<name<<endl;
				}
};
int main (){
	student test_student("pooja");
	student unknown_test_student;
	test_student.printName();
	unknown_test_student.printName();
}
