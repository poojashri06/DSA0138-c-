# poojashir
#include <iostream> 
using namespace std;
int main()
{
 int num = 12;
 int copyNum = num;
 int total = 0;

 // Add Each digit from last digit
 while(copyNum != 0)
 {
  total += copyNum % 10;
  copyNum = copyNum / 10;
 }
 // result
 cout << "The sum of digit " << num <<": " << total;

 return 0;
}
