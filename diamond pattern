# poojashri
#include<iostream>
using namespace std;

void printDiamond( int n ){
    int i,j,s;

    for(i=1;i<=n;i++){
        for(s=1;s<=n-i;s++)
             cout<<" ";

        for(j=1;j<=i*2-1;j++)
             cout<<"*";

        cout<<endl;
    }

    for(i=n;i>=1;i--){

         for(s=1;s<=n-i;s++)
            cout<<" ";

        for(j=1;j<=i*2-1;j++)
            cout<<"*";

        cout<<endl;
    }
  }

int main()
{

    printDiamond( 5 );

    return 0;
  }
